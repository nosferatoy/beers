		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-select": "^3.1.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"npm run lint",
			"npm run test",
			"git add"
